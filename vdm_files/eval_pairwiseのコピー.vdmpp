class sampleClass

functions

-- arg    a:int, b:nat
-- return seq of char
-- 戻り値の文字列には、その行が実行される際に引数aが満たす条件を記述した
-- ツールの処理と文字列の内容は無関係である

sampleFunction : int*nat*nat -> seq of char
sampleFunction(a, b, c) ==
    if(a < 100) then
      if(b > 2018) then
        "aは100未満かつbは2018より大きい"
      else
        "aは100未満かつbは2018以下"
    elseif(c < 12) then
      "aは100以上かつcは12未満"
    else
      "aは100以上かつcは12以上";

end sampleClass
